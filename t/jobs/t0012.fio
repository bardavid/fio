# Expected results: no parse warnings, runs and with roughly 1:5:10 iops between
#			the three jobs.
# Buggy result: parse warning on flow value overflow, no 1:5:10 division between
#			jobs.
#

[global]
bs=4k
ioengine=null
size=100g
runtime=10
flow_id=1
gtod_cpu=1
flow_sleep=100
rate_iops=200
thread

[flow1]
flow=1

[flow2]
flow=5

[flow3]
flow=10
